<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Member.
    -->
    <changeSet id="20191031140955-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="member"
                                 constraintName="fk_member_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="member"/>

        <addForeignKeyConstraint baseColumnNames="mentor_id"
                                 baseTableName="member"
                                 constraintName="fk_member_mentor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mentor"/>

    </changeSet>
</databaseChangeLog>
